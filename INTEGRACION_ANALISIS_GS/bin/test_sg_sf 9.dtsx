<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{9558AA1F-D8DE-4A81-80B3-B65444829AC3}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="  &#xA;--si isClosed es 1 o es certificacion de calidad (a veces aparecen con isClosed=0)  y el stc_iid &gt;0 (existe)  hay que cerrar el caso en softguard&#xA;if ( ?='Certificación de Calidad' or ?=1 ) and ?&gt;0&#xA;begin&#xA;--------------------Actualizamos el estado a Cerrado------------&#xA;update _Datos.dbo.m_st_cabecera&#xA;set stc_nestado=4 -- finalizado&#xA;--,stc_dfecha_modificacion=getdate()&#xA;,[stc_dfecha_cierre]=getdate()&#xA;&#xA;where stc_iid=?&#xA;--------------------------------------------------------------------------------------------&#xA;&#xA;      &#xA;        -------------Actualizamos su observación, si no existe la creamos-----------------------&#xA;            if ?&gt;0  and  (select count(*) from [_Datos].[dbo].[SerTecTimeLine] where stl_iServicio=?  and stl_cAccion='Observacion' and stl_cObservacion like '%Salesforce]%')=0 --revisa si existe observacion&#xA;  	  begin   &#xA;           		 insert into  [_Datos].[dbo].[SerTecTimeLine] (&#xA;       		[stl_iServicio]&#xA;      		,[stl_tFechaHora]&#xA;      		,[stl_cAccion]&#xA;     		 ,[stl_cObservacion]&#xA;    		  ,[stl_iUsuarioDSS])&#xA;      &#xA;     		 values &#xA;      &#xA;     		 (?,getdate(),'Observacion',?,23)&#xA;&#xA;   	 end    &#xA;            else&#xA;                 begin&#xA; 	 update [_Datos].[dbo].[SerTecTimeLine]&#xA;                    set&#xA;     	stl_tFechaHora=getdate(),stl_cObservacion=?-- actualizamos el comentario&#xA;    &#xA;     	where stl_iServicio=? and stl_cObservacion like '%Salesforce]%' and stl_cAccion='Observacion'&#xA;                end&#xA;      ----------------------------&#xA;&#xA;&#xA; &#xA;end&#xA;&#xA;else -- si no ha finalizado, solo actualizar el campo de observacion&#xA;   begin&#xA; if ?&gt;0 --stc_iid debe existir&#xA;    begin&#xA;       if   (select count(*) from [_Datos].[dbo].[SerTecTimeLine] where stl_iServicio=?  and stl_cAccion='Observacion' and stl_cObservacion like '%Salesforce]%')=0 --revisa si existe observacion&#xA;    begin   &#xA;            insert into  [_Datos].[dbo].[SerTecTimeLine] (&#xA;       [stl_iServicio]&#xA;      ,[stl_tFechaHora]&#xA;      ,[stl_cAccion]&#xA;      ,[stl_cObservacion]&#xA;      ,[stl_iUsuarioDSS])&#xA;      &#xA;      values &#xA;      &#xA;      (?,getdate(),'Observacion',?,23)&#xA;&#xA;    end    &#xA;    else begin&#xA;  update [_Datos].[dbo].[SerTecTimeLine]&#xA;      set&#xA;     stl_tFechaHora=getdate(),stl_cObservacion=?-- actualizamos el comentario&#xA;    &#xA;     where stl_iServicio=? and stl_cObservacion like '%Salesforce]%' and stl_cAccion='Observacion'&#xA;     end&#xA;  &#xA;    end &#xA;&#xA;end&#xA;&#xA;&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"><SQLTask:ParameterBinding SQLTask:ParameterName="13" SQLTask:DtsVariableName="User::desc_antiguos" SQLTask:ParameterDirection="Input" SQLTask:DataType="129" SQLTask:ParameterSize="5000"/><SQLTask:ParameterBinding SQLTask:ParameterName="1" SQLTask:DtsVariableName="User::is_closed_antiguos" SQLTask:ParameterDirection="Input" SQLTask:DataType="11" SQLTask:ParameterSize="-1"/><SQLTask:ParameterBinding SQLTask:ParameterName="2" SQLTask:DtsVariableName="User::stc_iid_antiguos" SQLTask:ParameterDirection="Input" SQLTask:DataType="131" SQLTask:ParameterSize="-1"/><SQLTask:ParameterBinding SQLTask:ParameterName="3" SQLTask:DtsVariableName="User::stc_iid_antiguos" SQLTask:ParameterDirection="Input" SQLTask:DataType="131" SQLTask:ParameterSize="-1"/><SQLTask:ParameterBinding SQLTask:ParameterName="10" SQLTask:DtsVariableName="User::stc_iid_antiguos" SQLTask:ParameterDirection="Input" SQLTask:DataType="131" SQLTask:Param