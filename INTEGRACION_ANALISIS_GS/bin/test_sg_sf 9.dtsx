<property id="30" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">-- A esta altura ya estan creados los casos nuevos en salesforce, ahora corresponde actualizar en softguard
--Tomamos todos casos pendientes excepto aquellos que no hay que llevar a salesforce (seran cancelados en softguard)


select a.*,casos.N_mero_caso_softguard__c,casos.CaseNumber,convert(int,casos.caseNumber) as CaseNumberInt, casos.Status,casos.IsClosed

   ,Case when Status not in ('Cerrado','Cerrado - primera llamada') then 1 else 4 end as 'Estado' -- Consultar los estados
  , '[Salesforce] Estado Actual: '+ ISNULL (CONVERT(varchar(60), Status),'- ')+
   ', Fecha Inicio Programacion: '+ISNULL (  CONVERT(varchar,Fecha_programada_de_inicio__c,103),'- ')+' '+ISNULL ( CONVERT(varchar,Fecha_programada_de_inicio__c,108),'')+
   ', Fecha Termino Programacion: '+ISNULL (  CONVERT(varchar,Fecha_programada_de_termino__c,103),'- ')+' '+ISNULL (  CONVERT(varchar,Fecha_programada_de_termino__c,108),'')+
   ', Fecha Inicio: '+ISNULL ( CONVERT(varchar,Fecha_de_inicio__c,103),'- ')+' '+ISNULL ( CONVERT(varchar,Fecha_de_inicio__c,108),'')+
   ', Fecha Termino: '+ISNULL ( CONVERT(varchar,Fecha_de_termino__c,103),'- ')+' '+ISNULL ( CONVERT(varchar,Fecha_de_termino__c,108),'- ')+
   ', Tecnico Asignado: '+ISNULL ( CONVERT(varchar(100),Tecnico_asignado__c),'- ')+
