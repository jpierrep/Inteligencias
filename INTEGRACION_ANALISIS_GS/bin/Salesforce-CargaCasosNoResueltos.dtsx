-- el caso debe estar abierto pues si esta cerrado ya se trató el tema, si siguen teniendo nsr se creará nuevamente el caso


select 
cas.id as IdCaso
,cue.id as IdCuenta

-- si se reestableció el cliente se actualiza el campo descripcion para alertar, si aun tiene mas de 3 dias se actualiza la descripcion con los dias
--si el estado es otro, se actualiza dicho estado en el comentario

,case when cue.Estado_recepciones_monitoreo__c='Cliente con Señales' then '[Actualización: Cliente con señales reestablecidas] '+convert(varchar(max),cas.[Description])
else
case when 

case when isnumeric(SUBSTRING(Estado_recepciones_monitoreo__c,1,2))=1 then convert(int,SUBSTRING(Estado_recepciones_monitoreo__c,1,2)) else 0 end &gt;3  then 
'Cliente con más de 3 días con NSR ('+convert(varchar,convert(int,SUBSTRING(cue.Estado_recepciones_monitoreo__c,1,2)))+'+)'
 else '[Actualización: Cuenta con '+Estado_recepciones_monitoreo__c+'] '+convert(varchar(max),cas.[Description])  end
end as 'Descipcion'
,Estado_recepciones_monitoreo__c
  ,Casos_tecn_abiertos__c
  ,Deuda__c
  ,Estado_Cuenta_Monitoreo__c
  ,Estado_de_cuenta_facturaci_n__c,Estado_de_cuenta__c
  ,Casos_tecn_3_meses__c
  ,'012C0000000MkhTIAS' as RecordType
  
 FROM [Inteligencias].[dbo].[SF_CASOS]  as cas
  inner join Inteligencias.dbo.SF_CUENTAS as cue
 on cas.AccountId=cue.Id

where
 case when isnumeric(SUBSTRING(Estado_recepciones_monitoreo__c,1,2))=1 then convert(int,SUBSTRING(Estado_recepciones_monitoreo__c,1,2)) else 0 end &gt;3 and
 RecordTypeId='012C0000000MkhTIAS'  

and
 cas.isClosed=0
 and cas.isDeleted=0</property>
