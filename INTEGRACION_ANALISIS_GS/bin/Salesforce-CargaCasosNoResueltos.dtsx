<property id="3" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="4" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">--crear caso nuevo , cerrar el padre y setear el numero del caso padre al hijo, para seguimiento



SELECT 
  
      cue.Estado_recepciones_monitoreo__c
      ,Case when cue.Deuda__c&gt;65000 then 'Deuda mas de 65000' end  as Deuda
      ,Case when RecordTypeId='012C0000000UqtdIAC' then 'casoSG' end as casoSG
      ,cue.Casos_tecn_abiertos__c
      ,(select top 1 c.createdDate from inteligencias.dbo.sf_casos as c where c.createdDate&gt;cas.createdDate and c.accountid=cas.accountid) as fecha_casos_posterior
      ,cas.createddate
      ,cas.AccountId
      ,cas.ContactId
      ,'012C0000000MkhsIAC' as 'RecordTypeId' -- casos cert no aprobada
      ,'Caso anterior inconcluso' as [Motivo_del_caso__c]
      ,cas.id as 'parent_case_id'
      ,isnull([Tecnico_que_realizo_trabajos__c],'') as 'Tecnico_que_realiz__c' --tecnico que realizo trabajos caso anterior se pasa al campo que en vista salesforce se llama tecnico caso anterior (Tecnico_que_realiz__c) en api
      ,isnull([Priority],'')  as 'Prioridad'
      ,'Caso anterior no resuelto'      as [Origin]
      ,'Caso anterior certificacion calidad no aprobada' as 'Subject'
      ,'Info caso anterior certificacion calidad no aprobada [Código] '+isnull(cas.caseNumber,'-')+' [Técnico] '+isnull([Tecnico_asignado__c],'-')+' [Asunto] '+ isnull([Subject],'-')
      +' [Descripción] '+convert(varchar(1000),isnull(cas.[Description],'-'))+'... [Resolución] '+convert(varchar(1000),isnull([Resoluci_n_de_Caso__c],'-'))+'...' as 'Description'
    
      
  FROM [Inteligencias].[dbo].[SF_CASOS]  as cas
  inner join Inteligencias.dbo.SF_CUENTAS as cue
 on cas.AccountId=cue.Id

where --[Se_requiere_abrir_otro_caso__c] ='SI' 
 
 --certificacione de calidad requiere abrir otro caso
 Certificacion_indica_crear_otro_casi__c=1
and cas.isDeleted=0  
and cas.isCLosed=1


 -- que no tenga otro caso posterior creado
and (select top 1 c.createdDate from inteligencias.dbo.sf_casos as c where c.createdDate&gt;cas.createdDate and c.accountid=cas.accountid) is null
--que no tenga otro caso abierto
and (select COUNT(*) from inteligencias.dbo.sf_casos as c where c.accountid=cas.accountid and c.IsClosed=0 and c.IsDeleted=0)=0
 --fecha tope para creacion de casos 
and cas.createddate&gt;'20170401'</property>
