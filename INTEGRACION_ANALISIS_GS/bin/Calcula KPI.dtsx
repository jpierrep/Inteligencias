<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E579E380-D90B-4A42-8B87-7F24D6ACD033}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;update SISTEMA_CENTRAL.dbo.metricas_bi&#xA;&#xA;set metrica_valor=metr.METRICA_VALOR&#xA;&#xA;&#xA;from (&#xA;------------------------------------------&#xA; &#xA;--contiene inasistencias sin personal asignado, incluye relevos no asignados&#xA;select b.EMP_CODI,b.CENCO2_CODI, case when CANT_TURNOS_NO_ASIG IS null then 0 else CANT_TURNOS_NO_ASIG end as 'METRICA_VALOR'&#xA;from&#xA;(&#xA;SELECT  EMPRESA_CODI_ASIST as 'EMP_CODI', CENCO2_CODI_ASIST as 'CENCO2_CODI' , COUNT(*) as 'CANT_TURNOS_NO_ASIG'&#xA;&#xA;  FROM [Inteligencias].[dbo].[GS_ASISTENCIASv2] &#xA;  inner join Inteligencias.dbo.GS_PERSONAL_ASISTENCIASv2 as per&#xA;  &#xA;  -- Solo Personal activo, turnos mayor que 0 y sin reemplazo&#xA;  on per.Id_Detalle=GS_ASISTENCIASv2.FUNC and per.DetalleFuncActivo='SI' and IsPersonalTunoActivo=1&#xA;  &#xA;  where &#xA;  &#xA;  TIPO_HORA='HH INASISTENCIA' and &#xA;  MOTIVO_DESC not in ('VACACIONES CON RELEVO','FINIQUITADO CON RELEVO','LICENCIA MEDICA CON RELEVO')  and                    &#xA;  REEMPLAZO is null and HORAS_CANT&gt;0 &#xA;  &#xA;&#xA;  &#xA;  --(MOTIVO_DESC ='RELEVO NO ASIGNADO'&#xA; -- or (TIPO_HORA='HORA INASISTENCIA' and REEMPLAZO is null )&#xA;  &#xA; --and FECHA_ASIST between GETDATE() and DATEADD(d,30,GETDATE())  -- 30 dias corridos&#xA;-- cambio 08/03/2018 se tomaba todo el mes completo desde la fecha actual, ahora contamos desde el primer dia del mes a la fecha  &#xA;-- and FECHA_ASIST between convert(date,getdate()) and DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,GETDATE())+1,0)) -- fecha entre el dia de hoy y el ultimo dia del mes&#xA;and FECHA_ASIST between DATEADD(mm,DATEDIFF(mm,0,GETDATE()),0) and convert(date,getdate())  &#xA;&#xA; group by EMPRESA_CODI_ASIST, CENCO2_CODI_ASIST )a&#xA; &#xA; &#xA; &#xA;  right join &#xA; --join con todos los centros de costo de asistencia&#xA; &#xA; (select distinct  &#xA; EMPRESA_CODI_ASIST as 'EMP_CODI', CENCO2_CODI_ASIST as 'CENCO2_CODI'&#xA;&#xA;from Inteligencias.dbo.GS_ASISTENCIASv2&#xA;where PRESUP_REAL='REAL' and IsPersonalTunoActivo=1 and FECHA_ASIST between DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()), 0) --desde comienzo de mes a 30 dias ya que puede haber calendarios sin dotacion en el rango 30 dias&#xA;&#xA; and DATEADD(d,30,GETDATE())&#xA; )b&#xA; &#xA; on a.CENCO2_CODI=b.CENCO2_CODI and a.EMP_CODI=b.EMP_CODI&#xA; &#xA; -------------------------------------------------------&#xA; &#xA; ) metr&#xA;&#xA;where metricas_bi.metrica_id = 5 and metricas_bi.centro_costo collate SQL_Latin1_General_CP1_CI_AI=metr.CENCO2_CODI and metricas_bi.empresa=metr.EMP_CODI and &#xA;mes=MONTH(GETDATE()) and año=YEAR(getdate()) " SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
