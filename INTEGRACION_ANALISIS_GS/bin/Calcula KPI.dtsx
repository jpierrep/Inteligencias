<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E579E380-D90B-4A42-8B87-7F24D6ACD033}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="  update SISTEMA_CENTRAL.dbo.metricas_bi_supervisor &#xA;  set metrica_valor=a.metrica_valor&#xA;  &#xA;  from (&#xA;  select 6 as 'Metrica_id','Cert. S/Info' as 'Metrica_desc',ADMINISTRATIVO_ID as administrativo_id,COUNT(*) as 'metrica_valor' from  [Inteligencias].[dbo].[CRED_RESUMEN] as acre&#xA;  where CERTIFICADO_ESTADO='SIN INFORMACION'&#xA;   group by ADMINISTRATIVO_ID &#xA;  union&#xA;    select 7,'Cred. S/Info o S/Solic.', ADMINISTRATIVO_ID,COUNT(*) from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CREDENCIAL_ESTADO='SIN INFORMACION' or CREDENCIAL_ESTADO='no_solicitada' group by ADMINISTRATIVO_ID &#xA; &#xA; union&#xA; select 8, 'Cert. Vencido' , ADMINISTRATIVO_ID,COUNT(*) as 'Cantidad' from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CERTIFICADO_ESTADO='vencido' group by ADMINISTRATIVO_ID &#xA;  union &#xA;    select 9,'Cred. Vencida', ADMINISTRATIVO_ID,COUNT(*) from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CREDENCIAL_ESTADO='vencida' group by ADMINISTRATIVO_ID &#xA;   union&#xA;   &#xA; select 10, 'Cert. en Trámite' , ADMINISTRATIVO_ID,COUNT(*) as 'Cantidad' from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CERTIFICADO_ESTADO in ('formacion','reentrenamiento') group by ADMINISTRATIVO_ID &#xA;  union &#xA;    select 11,'Cred. en Trámite', ADMINISTRATIVO_ID,COUNT(*) from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CREDENCIAL_ESTADO in ('en_tramite') group by ADMINISTRATIVO_ID &#xA;   union&#xA; select 12 ,'Cert. 30 días Venc.', ADMINISTRATIVO_ID,COUNT(*) as 'Cantidad' from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CERT_DIAS_VENC between 1 and 30 group by ADMINISTRATIVO_ID &#xA;  union &#xA;    select 13,'Cred. 30 días Venc.', ADMINISTRATIVO_ID,COUNT(*) from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CRED_DIAS_VENC between 1 and 30 group by ADMINISTRATIVO_ID &#xA;  &#xA;     union&#xA; select 14 ,'Cert. 60 días Venc.', ADMINISTRATIVO_ID,COUNT(*) as 'Cantidad' from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CERT_DIAS_VENC between 31 and 60 group by ADMINISTRATIVO_ID &#xA;  union &#xA;    select 15,'Cred. 60 días Venc.', ADMINISTRATIVO_ID,COUNT(*) from  [Inteligencias].[dbo].[CRED_RESUMEN]&#xA;  where CRED_DIAS_VENC between 31 and 60 group by ADMINISTRATIVO_ID &#xA;  &#xA;  ) a&#xA;  &#xA;  where  mes=MONTH(GETDATE()) and año=YEAR(getdate()) &#xA;  and a.administrativo_id=metricas_bi_supervisor.administrativo_id and a.Metrica_id=metricas_bi_supervisor.metrica_id&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
