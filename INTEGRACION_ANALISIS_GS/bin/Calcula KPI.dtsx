<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E579E380-D90B-4A42-8B87-7F24D6ACD033}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="  update SISTEMA_CENTRAL.dbo.metricas_bi_supervisor &#xA;  set metrica_valor=a.metrica_valor&#xA;  &#xA;  from (&#xA;&#xA;SELECT &#xA;case when TIPO in ('MULTA LABORAL','JUICIOS LABORALES') then 16 when TIPO ='MULTA COMERCIAL' then 17 when TIPO='J.P.L (OS10)' then 18 end as 'Metrica_id'&#xA;,case when TIPO in ('MULTA LABORAL','JUICIOS LABORALES') then 'Multas laborales (M$)' when TIPO ='MULTA COMERCIAL' then 'Multas Comerciales (M$)'&#xA;when TIPO='J.P.L (OS10)' then 'Multas OS10 (M$)'end as 'Metrica_desc'&#xA;,estr.administrativo_id,round(sum( isnull( TOTAL_CURSADO,0))/1000000,2) as 'Metrica_valor'--multas en millones&#xA;&#xA;  FROM [Inteligencias].[dbo].[MULTAS]&#xA;  &#xA;  left join Inteligencias.dbo.SIST_CENTRAL_ESTR_ORGANIZACION as estr &#xA;on estr.cencos_codigo collate SQL_Latin1_General_CP1_CI_AI=MULTAS.CENCO2_CODI and estr.empresa_id=MULTAS.EMP_CODI&#xA;  where year(FECHA_INFRACCION)=YEAR(getdate()) and empresa_id=0 and estr.administrativo_id is not null&#xA;and estr.cencos_codigo collate SQL_Latin1_General_CP1_CI_AI=MULTAS.CENCO2_CODI and estr.empresa_id=MULTAS.EMP_CODI&#xA;&#xA;  &#xA;  group by estr.administrativo_id&#xA;,case when TIPO in ('MULTA LABORAL','JUICIOS LABORALES') then 16 when TIPO ='MULTA COMERCIAL' then 17 when TIPO='J.P.L (OS10)' then 18 end&#xA;,case when TIPO in ('MULTA LABORAL','JUICIOS LABORALES') then 'Multas laborales (M$)' when TIPO ='MULTA COMERCIAL' then 'Multas Comerciales (M$)'&#xA;when TIPO='J.P.L (OS10)' then 'Multas OS10 (M$)'end &#xA;&#xA;  ) a&#xA;  &#xA;  where  mes=MONTH(GETDATE()) and año=YEAR(getdate()) &#xA;  and a.administrativo_id=metricas_bi_supervisor.administrativo_id and a.Metrica_id=metricas_bi_supervisor.metrica_id&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
