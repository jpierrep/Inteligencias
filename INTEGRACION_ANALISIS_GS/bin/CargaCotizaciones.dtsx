<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="---TENER EN CUENTA QUE LOS OTROS GASTOS NO SE ENCUENTRAN SEPARADOS POR CARGO&#xA;&#xA;&#xA;&#xA;------------FACTURACION---------------------&#xA;&#xA;truncate table COTIZACION_FACTURACION &#xA;GO&#xA;&#xA;SET IDENTITY_INSERT COTIZACION_FACTURACION  ON&#xA;&#xA;insert into COTIZACION_FACTURACION &#xA;([IdFacturacion]&#xA;      ,[Cotizacion]&#xA;      ,[Columna]&#xA;      ,[Personal]&#xA;      ,[CostoEmpresa]&#xA;      ,[Utilidad]&#xA;      ,[Gasto]&#xA;      ,[Costo]&#xA;      ,[Venta]&#xA;      ,[HoraNormal]&#xA;      ,[HoraExtra]&#xA;      ,[ValorUnitario])&#xA;&#xA;select [IdFacturacion],Facturacion.[Cotizacion],Facturacion.[Columna],Facturacion.[Personal],Facturacion.[CostoEmpresa],Facturacion.[Utilidad]&#xA;,Facturacion.[Gasto],Facturacion.[Costo],Facturacion.[Venta],Facturacion.[HoraNormal],Facturacion.[HoraExtra],Facturacion.[ValorUnitario]&#xA; from Sistemas.dbo.Facturacion inner join Sistemas.dbo.Cotizacion&#xA;on Cotizacion.IdCotizacion=Facturacion.Cotizacion&#xA;where Cotizacion.Estado IN (0, 1, 9, 19)&#xA;GO&#xA;&#xA;SET IDENTITY_INSERT COTIZACION_FACTURACION  OFF&#xA;&#xA;exec CalculaTotalesFacturacion&#xA;GO&#xA;&#xA;------------COSTOS---------------------&#xA;&#xA;------------FACTURACION---------------------&#xA;&#xA;truncate table COTIZACIONES_COSTOS&#xA;GO&#xA;&#xA;SET IDENTITY_INSERT COTIZACIONES_COSTOS ON&#xA;&#xA;insert into COTIZACIONES_COSTOS&#xA;([IdCosto],[Cotizacion],[Simulacion],[Columna],[Titulo],[Codigo],[SueldoBase],[SueldoInicio],[Gratificacion],[Bono1],[Bono2],[Bono3],[BonoCaja]&#xA;,[BonoHerramienta],[Movilización],[Colación],[Liquido],[Mutual],[Indemnizacion],[Vacaciones],[SeguroVida],[Invalidez],[Festivo],[Uniforme],[Utiles]&#xA;,[OS10],[Cesantia],[Dotacion],[OtroGasto],[Segmento],[PorcentajeIAS],[CostoColacion])&#xA;      &#xA;select [IdCosto],Costos.[Cotizacion],Costos.[Simulacion],Costos.[Columna],Costos.[Titulo],Costos.[Codigo],Costos.[SueldoBase],Costos.[SueldoInicio]&#xA;,Costos.[Gratificacion],Costos.[Bono1],Costos.[Bono2],Costos.[Bono3],Costos.[BonoCaja],Costos.[BonoHerramienta],Costos.[Movilización]&#xA;,Costos.[Colación],Costos.[Liquido],Costos.[Mutual],Costos.[Indemnizacion],Costos.[Vacaciones],Costos.[SeguroVida],Costos.[Invalidez],Costos.[Festivo]&#xA;,Costos.[Uniforme],Costos.[Utiles],Costos.[OS10],Costos.[Cesantia],Costos.[Dotacion],Costos.[OtroGasto],Costos.[Segmento],Costos.[PorcentajeIAS]&#xA;,Costos.[CostoColacion]&#xA;&#xA; from Sistemas.dbo.Costos&#xA;inner join Sistemas.dbo.Cotizacion&#xA;on Cotizacion.IdCotizacion=Costos.Cotizacion&#xA;where Cotizacion.Estado IN (0, 1, 9, 19)&#xA;&#xA;GO&#xA;&#xA;SET IDENTITY_INSERT COTIZACIONES_COSTOS  OFF&#xA;&#xA;exec CalculaTotalesCostos&#xA;GO&#xA;&#xA;&#xA;&#xA;&#xA;--------------------------------------OTROS GASTOS-----------------------------------------------&#xA;&#xA;-- Cargamos cuenta para agrupar otros gastos no clasificados con cuentas contables 09/11/2017&#xA;&#xA;delete from [TARGIT_GESTION_GS].[dbo].CUENTAS &#xA;where CTA3_CODI in ('50-01-888')&#xA;&#xA;INSERT &#xA;  INTO [TARGIT_GESTION_GS].[dbo].CUENTAS(CTA1_CODI,CTA2_CODI,CTA3_CODI)&#xA;SELECT '50-00-000','50-01-000','50-01-888'&#xA;&#xA;GO&#xA;delete from [TARGIT_GESTION_GS].[dbo].CUENTAS_N3 &#xA;where CTA3_CODI in ('50-01-888')&#xA;INSERT&#xA;  INTO [TARGIT_GESTION_GS].[dbo].CUENTAS_N3(CTA3_CODI,CTA3_DESC)&#xA;SELECT '50-01-888', 'OTROS GASTOS COTIZACION S/CATEGORIA'&#xA;&#xA;--carga apertura funcional&#xA;  delete from &#xA;  [TARGIT_GESTION_GS].[dbo].[CUENTAS_APERTURA_FUNC] where CTA3_DESC='OTROS GASTOS COTIZACION S/CATEGORIA'&#xA;  &#xA;  insert into [TARGIT_GESTION_GS].[dbo].[CUENTAS_APERTURA_FUNC]&#xA;  ([CTA1_DESC],[CTA3_DESC],[CTA1_CODI],[CTA2_CODI],[CTA3_CODI],[CTA2_DESC],[Copy of AGR_BAL3_DESC],[APERTURA_FLUJO_CAJA])&#xA;  select &#xA;  'GASTOS','OTROS GASTOS COTIZACION S/CATEGORIA','50-00-000','50-01-000','50-01-888','COSTOS DE EXPLOTACION','OTROS','OTROS DESEMBOLSOS'&#xA;&#xA;--En otros gastos se cruza con las cuentas contables, si no hay cruce, se añade a la cuenta creada para agrupar aquellos que no tienen clasificacion&#xA;&#xA;truncate table Inteligencias.dbo.COTIZACION_OTRO_GASTOS&#xA;insert into Inteligencias.dbo.COTIZACION_OTRO_GASTOS&#xA;SELECT        IdTabla, Cotizacion, Columna, Fila, Segmento, Gasto, Valor, Costo, Total, case when IT.[CODIGO CUENTA] is not null then  cast(IT.[CODIGO CUENTA] as varchar (18)) else '50-01-888' end as CTA3_CODI&#xA;,COTIZACION.Centro as CENCO2_CODI,(select top 1 area_codi from inteligencias.dbo.mantenedor_cc_area  as mant where  mant.cenco2_codi collate Modern_Spanish_CI_AS=COTIZACION.Centro and mant.emp_codi=cotizacion.Empresa ) as AREA_CODI,COTIZACION.Empresa as EMP_CODI&#xA;&#xA;FROM            Sistemas.dbo.OtroGasto as OtroG&#xA;inner join Sistemas.dbo.Cotizacion&#xA;on Cotizacion.IdCotizacion=OtroG.Cotizacion&#xA;&#xA; left join Inteligencias.dbo.COTIZACION_ITEM_CUENTA  as IT&#xA;&#xA;&#xA;&#xA;on  IT.ITEM collate Modern_Spanish_CI_AS=OtroG.Gasto&#xA;WHERE        (Valor &gt; 0) and Cotizacion.Estado IN (0, 1, 9, 19)&#xA;&#xA;&#xA;&#xA;---------------PORCENTAJES--------------------------------&#xA;&#xA;&#xA;SET IDENTITY_INSERT [Inteligencias].[dbo].[COTIZACIONES_PORCENTAJE] ON&#xA;&#xA;truncate table [Inteligencias].[dbo].[COTIZACIONES_PORCENTAJE]&#xA; insert into [Inteligencias].[dbo].[COTIZACIONES_PORCENTAJE]&#xA; ([IdPorcentaje]&#xA;      ,[Cotizacion]&#xA;      ,[Columna]&#xA;      ,[Gasto]&#xA;      ,[Utilidad]&#xA;      ,[Ordinaria]&#xA;      ,[Extra])&#xA; select [IdPorcentaje]&#xA;      ,[Cotizacion]&#xA;      ,[Columna]&#xA;      ,[Gasto]&#xA;      ,[Utilidad]&#xA;      ,[Ordinaria]&#xA;      ,[Extra]&#xA;  FROM [Sistemas].[dbo].[Porcentaje] &#xA;  &#xA;  inner join Sistemas.dbo.Cotizacion&#xA;on Cotizacion.IdCotizacion=Porcentaje.Cotizacion&#xA;&#xA;where Cotizacion.Estado IN (0, 1, 9, 19)&#xA; &#xA;  SET IDENTITY_INSERT [Inteligencias].[dbo].[COTIZACIONES_PORCENTAJE] OFF&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
