<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="--Actualizamos Totales por cargo (medida para agrupacion de cotizaciones para que sumen los totales y no los multipliquen por la dotacion&#xA;&#xA;update&#xA;&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;set [COTIZACION_COSTOS].Monto_por_Cargo=a.Monto_por_Cargo&#xA;from &#xA;(select p.Cotizacion,p.Tipo,sum(p.Monto_por_Cargo) as Monto_por_Cargo  from&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS] as p where p.Titulo &lt;&gt;'Totales'&#xA;  group by p.Cotizacion, p.Tipo&#xA;) a&#xA;&#xA;where [COTIZACION_COSTOS].Cotizacion=a.Cotizacion and  [COTIZACION_COSTOS].Tipo=a.Tipo and  [COTIZACION_COSTOS].Tipo not in('Dotacion','Porc Gastos Generales %','Porc Utilidad %') &#xA;and [COTIZACION_COSTOS].Titulo ='Totales'&#xA;&#xA;&#xA;--Hay ciertos items que su costo no se totaliza en base a la cantidad de personal,&#xA;--para ello debemos copiar los mismos valores de la medida monto a la medida monto_por_cargo&#xA;&#xA;update&#xA;&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;set Monto_por_Cargo=a.Monto&#xA;&#xA;from &#xA;(select * from&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]  &#xA;&#xA;) a&#xA;&#xA;where [COTIZACION_COSTOS].idRegistro=a.IdRegistro and&#xA;&#xA;[COTIZACION_COSTOS].Tipo &#xA;&#xA; in('Dotacion','Porc Gastos Generales %','Porc Utilidad %','OtroGasto','Precio Venta Mano Obra',&#xA; 'Fact Precio Mensual Total', 'Valor Proyecto Iva Incluido', 'Porc Valor Hora Extra Venta Neta',&#xA; 'Porc Valor Hora Ord. Venta Neta','Fact Precio Mensual Total c/IVA')&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
