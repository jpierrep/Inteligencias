<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E1898240-AA04-46F0-A1A1-AF17D90EF878}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="truncate table Inteligencias.dbo.COTIZACION&#xA;GO&#xA;SET IDENTITY_INSERT inteligencias.dbo.COTIZACION  ON&#xA;go&#xA;&#xA;insert into Inteligencias.dbo.COTIZACION&#xA;(&#xA;[IdCotizacion],[Empresa],[Area],[TECNOLOGIA],[Observacion],[TIPO COTIZACION],[TipoSub],[Estado],[ESTADO DESC]&#xA;,[Titulo],[Preparacion],[Fecha],[Mutual],[ValorGratificacion],[TIPO GRATIFICACION],[Cesantia],[Cliente],[Representante]&#xA;,[RutRep],[Centro],[Administrador],[FonoAdmin],[CelularAdmin],[Direccion],[Ciudad],[Email],[TipoServicio],[TipoJornada]&#xA;,[TIPO CONTRATO],[TIPO REAJUSTE],[Inicio],[HoraInicio],[Termino],[HoraTermino],[ValorServicio],[Facturacion],[EnUf],[ValorUf]&#xA;,[CantidadUf],[EMPCCOSTO],[EMP_CODI],[FECHA_ULTIMA_MOD],[FECHA_CREACION],[GRUPO],[FECHA_ULTIMA_FACT],[MONT_ULTIMA_FACT]&#xA;)&#xA;&#xA;SELECT        IdCotizacion, Empresa, Area, CASE (ConTecnologia) &#xA;                         WHEN 1 THEN 'CON TECN. Y SIN OTROS SERV.' WHEN 10 THEN 'SIN TECN. Y CON OTROS SERV.' WHEN 11 THEN 'CON TECN. Y CON OTROS SERV.' ELSE 'SIN TECN. y SIN OTROS SERV.'&#xA;                          END AS 'TECNOLOGIA', Observacion, CASE (TipoCotizacion) WHEN 0 THEN 'PERMANENTE' WHEN 1 THEN 'EVENTUAL' END AS 'TIPO COTIZACION', TipoSub, &#xA;                         Estado, &#xA;                         CASE Estado WHEN 0 THEN 'EN CREACION' WHEN 1 THEN 'APROBADA POR EL CLIENTE' WHEN 9 THEN 'ACTIVA SERVICIO PERMANENTE' WHEN 19 THEN 'ACTIVA SERVICIO EVENTUAL'&#xA;                          END AS 'ESTADO DESC', Titulo, Preparacion, Fecha, Mutual, ValorGratificacion, &#xA;                         CASE TipoCotizacion WHEN 1 THEN 'VALOR LEGAL' WHEN 0 THEN 'VALOR FIJO' END AS 'TIPO GRATIFICACION', Cesantia, Cliente, Representante, RutRep, &#xA;                         CASE WHEN Centro IS NULL THEN '' ELSE Centro END AS 'Centro', Administrador, FonoAdmin, CelularAdmin, Direccion, Ciudad, Email, TipoServicio, TipoJornada, &#xA;                         CASE TipoContrato WHEN 0 THEN 'PLAZO FIJO' WHEN 1 THEN 'PLAZO INDEFINIDO' ELSE 'SIN INFORMACION' END AS 'TIPO CONTRATO', &#xA;                         CASE TipoReajuste WHEN 0 THEN 'MENSUAL' WHEN 1 THEN 'BIMENSUAL' WHEN 2 THEN 'TRIMENSUAL' WHEN 3 THEN 'TRIMESTRAL' WHEN 4 THEN 'SEMESTRAL'&#xA;                          WHEN 5 THEN 'ANUAL' WHEN 6 THEN 'NEGOCIAR' WHEN 7 THEN 'NINGUNO' ELSE 'INDEFINIDO' END AS 'TIPO REAJUSTE', Inicio, HoraInicio, &#xA;                         CASE WHEN Termino IS NULL OR&#xA;                         YEAR(Termino) = 9999 THEN CONVERT(datetime, '99991231') ELSE Termino END AS 'Termino', HoraTermino, ValorServicio, Facturacion, EnUf, ValorUf, CantidadUf, &#xA;                         CAST(Empresa AS VARCHAR) + 'X' + CASE WHEN Centro IS NULL THEN '' ELSE Centro END AS EMPCCOSTO, CAST(Empresa AS int) AS EMP_CODI, &#xA;                         CASE WHEN&#xA;                             (SELECT        MAX(Fecha)&#xA;                               FROM            Inteligencias.dbo.COTIZACIONES_REPORTE AS REP&#xA;                               WHERE        Rep.IdCotCalculada = dbo.Cotizacion.IdCotizacion) IS NOT NULL THEN CONVERT(datetime, CONVERT(varchar,&#xA;                             (SELECT        MAX(Fecha)&#xA;                               FROM            Inteligencias.dbo.COTIZACIONES_REPORTE AS REP&#xA;                               WHERE        Rep.IdCotCalculada = dbo.Cotizacion.IdCotizacion), 102)) ELSE '9999-12-12' END AS FECHA_ULTIMA_MOD, CASE WHEN&#xA;                             (SELECT        Fecha&#xA;                               FROM            Inteligencias.dbo.COTIZACIONES_REPORTE AS REP&#xA;                               WHERE        Rep.IdCotCalculada = dbo.Cotizacion.IdCotizacion AND Rep.Descripcion = 'Crea Cotización') IS NOT NULL THEN CONVERT(datetime, &#xA;                         CONVERT(varchar,&#xA;                             (SELECT        Fecha&#xA;                               FROM            Inteligencias.dbo.COTIZACIONES_REPORTE AS REP&#xA;                               WHERE        Rep.IdCotCalculada = dbo.Cotizacion.IdCotizacion AND Rep.Descripcion = 'Crea Cotización'), 102)) ELSE '9999-12-12' END AS FECHA_CREACION, &#xA;                         CASE WHEN&#xA;                             (SELECT        TOP 1 g.Titulo&#xA;                               FROM            Relacion AS r INNER JOIN&#xA;                                                         Grupo AS g ON r.IdLider = g.IdGrupo&#xA;                               WHERE        r.IdAnexado = IdCotizacion) IS NULL THEN 'Sin Agrupacion' ELSE&#xA;                             (SELECT        TOP 1 g.Titulo&#xA;                               FROM            Relacion AS r INNER JOIN&#xA;                                                         Grupo AS g ON r.IdLider = g.IdGrupo&#xA;                               WHERE        r.IdAnexado = IdCotizacion) END AS 'GRUPO', CASE WHEN&#xA;                             (SELECT        FECHA&#xA;                               FROM            Inteligencias.dbo.FACTURACION_REAL_CC AS fac&#xA;                               WHERE        fac.CENCO2_CODI = Cotizacion.Centro AND fac.EMP_CODI = Cotizacion.Empresa) IS NULL THEN '' ELSE&#xA;                             (SELECT        FECHA&#xA;                               FROM            Inteligencias.dbo.FACTURACION_REAL_CC AS fac&#xA;                               WHERE        fac.CENCO2_CODI = Cotizacion.Centro AND fac.EMP_CODI = Cotizacion.Empresa AND Cotizacion.Estado IN (1, 9, 19)) END AS 'FECHA_ULTIMA_FACT', &#xA;                         CASE WHEN&#xA;                             (SELECT        FACTURACION&#xA;                               FROM            Inteligencias.dbo.FACTURACION_REAL_CC AS fac&#xA;                               WHERE        fac.CENCO2_CODI = Cotizacion.Centro AND fac.EMP_CODI = Cotizacion.Empresa) IS NULL OR&#xA;                         Cotizacion.Estado = 0 THEN 0 ELSE&#xA;                             (SELECT        FACTURACION&#xA;                               FROM            Inteligencias.dbo.FACTURACION_REAL_CC AS fac&#xA;                               WHERE        fac.CENCO2_CODI = Cotizacion.Centro AND fac.EMP_CODI = Cotizacion.Empresa) /&#xA;                             (SELECT        COUNT(*)&#xA;                               FROM            dbo.Cotizacion AS c&#xA;                               WHERE        Cotizacion.Centro = c.Centro AND c.Estado IN (1, 9, 19)) END AS 'MONT_ULTIMA_FACT'&#xA;&#xA;&#xA;FROM            Sistemas.dbo.Cotizacion&#xA;WHERE        (Estado IN (0, 1, 9, 19))&#xA;GO&#xA;&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
