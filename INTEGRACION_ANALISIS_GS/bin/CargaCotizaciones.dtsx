<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="--Llevamos Prov. Vacaciones e IAS, Gastos de Adm y Utilidad al calculo para medidas&#xA;&#xA;&#xA;&#xA;update&#xA;&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;set ProvIAS_por_Cargo=case when [COTIZACION_COSTOS].Tipo='Prov. Indemn. Años Serv.' then a.Monto_por_Cargo else 0 end &#xA;,ProvVAC_por_Cargo=case when [COTIZACION_COSTOS].Tipo='Provisión de Vacaciones' then a.Monto_por_Cargo else 0 end &#xA;, Utilidad_por_Cargo=case when [COTIZACION_COSTOS].Tipo='Fact Utilidad' then a.Monto_por_Cargo else 0 end &#xA;, GA_por_Cargo=case when [COTIZACION_COSTOS].Tipo='Fact Gasto de Administración' then a.Monto_por_Cargo else 0 end &#xA;from &#xA;(select * from&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]) a&#xA;&#xA;where [COTIZACION_COSTOS].IdRegistro=a.IdRegistro and [COTIZACION_COSTOS].Tipo not in('Dotacion','Porc Gastos Generales %','Porc Utilidad %') &#xA;and [COTIZACION_COSTOS].Titulo &lt;&gt;'Totales'" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
