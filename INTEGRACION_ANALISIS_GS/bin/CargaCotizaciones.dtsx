<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E1898240-AA04-46F0-A1A1-AF17D90EF878}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="truncate table Inteligencias.dbo.COTIZACIONES_REPORTE&#xA;&#xA;&#xA;insert into Inteligencias.dbo.COTIZACIONES_REPORTE&#xA;&#xA;SELECT        Rep.Usuario, Rep.Fecha, Rep.Tipo, substring(Rep.Observacion,1,200) as Observacion, &#xA;--obtenemos las modificaciones&#xA;Case when LEFT(Observacion,8)='Modificó' then 'Modifica Cotización' &#xA;--obtenemos las craciones&#xA;when LEFT(Observacion,4)='Creo' then 'Crea Cotización' &#xA;--Obtenemos los servicios terminados (debe decir si o si &quot;interno&quot; y comenzar con &quot;Se marco&quot;&#xA; when LEFT(Observacion,8)='Se marco' and PATINDEX ( '%interno%' , Observacion )&lt;&gt;0 then 'Servicio Terminado'&#xA; else  Acc.Descripcion end as Descripcion, &#xA;CASE WHEN Usuario.Nombre IS NULL &#xA;                         THEN 'Desconocido' ELSE Usuario.Nombre END AS NombreUsuario, &#xA;                         &#xA;                         --Aca calculamos el numero de la cotizacion &#xA;                         --Cuando no hay un &quot;se marcó&quot; en la descripcion quiere decir que es un caso nomal&#xA;                         -- obtenemos el id apartir del caracter 'º' y el espacio que hay luego ( aplica a char)&#xA;                         case when (left(Rep.Observacion,8) &lt;&gt;'Se marco') then &#xA;                         SUBSTRING(Rep.Observacion, CHARINDEX('º', Rep.Observacion) + 2, CHARINDEX(' ', &#xA;                         Rep.Observacion, &#xA;                        &#xA;                     &#xA;                         CHARINDEX('º', Rep.Observacion) + 2) - (CHARINDEX('º', Rep.Observacion) + 2)) &#xA;                         &#xA;                         -- Caso especial , servicios terminados. Debe existir en Observacion la palabra 'interno' y debe comenzar con 'Se marco'&#xA;                        -- claculamos en base a la palabra que antecede al id (interno) y las palabras que lo preceden (,como servicio)&#xA;                        when left(Rep.Observacion,8) ='Se marco' and PATINDEX ( '%interno%' , Observacion )&lt;&gt;0 then&#xA;                         &#xA;                        SUBSTRING(Observacion,  PATINDEX ( '%interno%' , Observacion )+7, &#xA;                        &#xA;                     &#xA;                        PATINDEX ( '%, como servicio%' , Observacion )-(PATINDEX ( '%interno%' , Observacion )+7)) &#xA;                        --si no hay coincidencia se deja en 0 el id&#xA;                        else 0&#xA;                        end&#xA;                       &#xA;                         AS IdCotCalculada, convert(datetime,convert(varchar, Fecha,102)) as FechaDia , row_number() OVER (ORDER BY &#xA;                             (SELECT        0)) as Id&#xA;&#xA;&#xA;FROM            dbo.Reporte AS Rep INNER JOIN&#xA;                         dbo.Acciones AS Acc ON Rep.Tipo = Acc.IdAccion LEFT OUTER JOIN&#xA;                         Invoicing.dbo.Usuario AS Usuario ON Usuario.Codigo = Rep.Usuario&#xA;WHERE        (ISNUMERIC(SUBSTRING(Rep.Observacion, CHARINDEX('º', Rep.Observacion) + 2, CHARINDEX(' ', Rep.Observacion, CHARINDEX('º', Rep.Observacion) + 2) &#xA;                         - (CHARINDEX('º', Rep.Observacion) + 2))) = 1) &#xA;                         &#xA;        --quitamos los id que tienen ','                 &#xA;delete from Inteligencias.dbo.COTIZACIONES_REPORTE where &#xA;&#xA;&#xA;IdCotCalculada not in (select  IdCotizacion from dbo.Cotizacion where Estado in (0, 1, 9, 19)) or&#xA;&#xA;&#xA; idCotCalculada like '%,%' or idCotCalculada=0  or idCotCalculada=2 " SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
