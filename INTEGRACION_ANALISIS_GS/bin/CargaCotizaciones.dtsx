<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="--CARGA COSTOS&#xA;&#xA;truncate table [Inteligencias].[dbo].[COTIZACION_COSTOS_GASTOS_EERR]&#xA;insert into [Inteligencias].[dbo].[COTIZACION_COSTOS_GASTOS_EERR]&#xA;SELECT  [Cotizacion]&#xA;      ,[IdCosto]&#xA;      ,[Titulo]&#xA;      ,[Tipo]&#xA;      ,[Monto]&#xA;      ,[IdRegistro]&#xA;      ,[CUENTA3_CODI]&#xA;      ,[Monto_por_Cargo]&#xA;      ,[Utilidad_por_Cargo]&#xA;      ,[GA_por_Cargo]&#xA;      ,[ProvIAS_por_Cargo]&#xA;      ,[ProvVAC_por_Cargo]&#xA; ,CENCO2_CODI,AREA_CODI,EMP_CODI&#xA;  FROM [Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;--- CARGA OTROS GASTOS&#xA;&#xA;&#xA;&#xA;insert into [Inteligencias].[dbo].[COTIZACION_COSTOS_GASTOS_EERR]&#xA;( [Cotizacion]&#xA;      ,[Titulo]&#xA;      ,[Tipo]&#xA;      ,[Monto_por_Cargo]&#xA;      ,[CUENTA3_CODI]&#xA;,CENCO2_CODI,AREA_CODI,EMP_CODI&#xA;)&#xA;  &#xA;&#xA;SELECT  &#xA;[Cotizacion]&#xA;,'Otros Gastos' as Titulo&#xA; ,[Gasto] as 'Tipo'&#xA; ,Total as 'Monto_por_Cargo'&#xA; ,[CTA3_CODI]&#xA; ,CENCO2_CODI,AREA_CODI,EMP_CODI&#xA; &#xA;  FROM [Inteligencias].[dbo].[COTIZACION_OTRO_GASTOS]&#xA;&#xA;&#xA;--AÑADE ID A TABLA&#xA;&#xA;    DECLARE @id BIGINT &#xA;SET @id = 0 &#xA;UPDATE [Inteligencias].[dbo].[COTIZACION_COSTOS_GASTOS_EERR]&#xA;SET @id = IdRegistro = @id + 1 &#xA;GO &#xA;&#xA;&#xA;&#xA;--actualiza toda la columna montos por cargo poniendo negativos los costos para homologar con EERR&#xA;&#xA;  update [Inteligencias].[dbo].[COTIZACION_COSTOS_GASTOS_EERR]&#xA;  set  Monto_por_Cargo =case when CUENTA3_CODI like '40-01-%' then Monto_por_Cargo else Monto_por_Cargo*-1 end&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
