<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="--El sistema calcula mal los totales de algunos items, los corregimos&#xA;&#xA;update&#xA;&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;set [COTIZACION_COSTOS].Monto=a.Monto&#xA;from &#xA;(select p.Cotizacion,p.Tipo,sum(p.Monto) as Monto from&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS] as p where p.Titulo &lt;&gt;'Totales'&#xA;  group by p.Cotizacion, p.Tipo&#xA;) a&#xA;&#xA;where [COTIZACION_COSTOS].Cotizacion=a.Cotizacion and  [COTIZACION_COSTOS].Tipo=a.Tipo and  [COTIZACION_COSTOS].Tipo  in('Precio Venta Mano Obra','Fact Precio Mensual Total','Valor Proyecto Iva Incluido','Fact Precio Mensual Total c/IVA') &#xA;and [COTIZACION_COSTOS].Titulo ='Totales'&#xA;&#xA;&#xA;--Calculamos los costos totalizados por  cargo y lo totalizamos en un medida&#xA;&#xA;update&#xA;&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS]&#xA;&#xA;set Monto_por_Cargo=[COTIZACION_COSTOS].monto*a.Monto&#xA;&#xA;from &#xA;(select * from&#xA;[Inteligencias].[dbo].[COTIZACION_COSTOS] where Tipo='Dotacion' ) a&#xA;&#xA;where [COTIZACION_COSTOS].Cotizacion=a.Cotizacion &#xA;and  [COTIZACION_COSTOS].Titulo=a.Titulo and&#xA;&#xA;[COTIZACION_COSTOS].Tipo not in('Dotacion','Porc Gastos Generales %','Porc Utilidad %')" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
