<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="truncate table  inteligencias.dbo.COTIZACION_DOTACION&#xA;&#xA;insert   &#xA;into inteligencias.dbo.COTIZACION_DOTACION&#xA;&#xA;SELECT &#xA;&#xA; [IdDotacion]&#xA;      ,[Cotizacion]&#xA;      ,cot.Empresa as Cotizacion_Empresa&#xA;      ,cot.Centro as Cotizacion_Centro&#xA;      ,[Linea]&#xA;      ,[Cargo]&#xA;      ,[Codigo]&#xA;      ,[Contratado]&#xA;      ,[Ubicacion]&#xA;      ,[Posicion]&#xA;      ,dot.[HoraInicio]&#xA;      ,dot.[HoraTermino]&#xA;      ,[Horas]&#xA;      ,[Extras]&#xA;      ,[Ciclo]&#xA;      ,[DiaInicio]&#xA;      ,[DiaTermino]&#xA;      ,dot.[Inicio]&#xA;      ,dot.[Termino]&#xA;      ,[Festivos]&#xA;      ,[Dias]&#xA;      ,[Factor]&#xA;      ,[Requerida]&#xA;      ,[Asignada]&#xA;      ,[Rol]&#xA;      ,[Colacion]&#xA;  &#xA;&#xA;  FROM [Sistemas].[dbo].[Dotacion] as dot&#xA;  left join sistemas.dbo.Cotizacion as cot&#xA;  &#xA;  on dot.Cotizacion=cot.IdCotizacion" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
