<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{90439A29-7650-4E3E-B925-D9CEAF902005}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;--OBTENEMOS LA FACTURACION MENSUAL POR CENTRO DE COSTO&#xA;&#xA;select&#xA;&#xA;CAST (DATEADD(month, DATEDIFF(month, 0, cpbfec), 0)as DATE) as Fecha ,&#xA;CcCod,&#xA;&#xA;SUM ( case when ( pctcod like '%40-01-002%' or pctcod like '%40-01-003%' ) then MovHaberMa-MovDebeMa else 0 end ) as Facturacion&#xA;into #facturacion&#xA;from &quot;GUARD&quot;.&quot;softland&quot;.&quot;cwmovim&quot; movimientos&#xA;--where cpbano &gt;= YEAR(GETDATE())&#xA;where cpbano = YEAR(DATEADD(m,-1,DATEADD(mm, DATEDIFF(m,0,GETDATE()), 0)))  --obtiene el año del mes anterior &#xA;and (MovhaberMa&gt;0 or MovDebeMa&gt;0)&#xA;and Cccod not like '001%'&#xA;group by DATEADD(month, DATEDIFF(month, 0, cpbfec), 0),CcCod&#xA;order by  DATEADD(month, DATEDIFF(month, 0, cpbfec), 0),CcCod&#xA;&#xA;-- obtenemos la maxima fecha con facturacion&#xA;select CcCod,MAX(fecha) as fecha&#xA;into #max&#xA;from #facturacion &#xA;where Facturacion&gt;0&#xA;&#xA;group by CcCod&#xA;&#xA;--obtenemos la facturacion de la maxima fecha en que se facturo&#xA;truncate table Inteligencias.dbo.FACTURACION_REAL_CC&#xA;insert into Inteligencias.dbo.FACTURACION_REAL_CC&#xA;select &#xA;0 as EMP_CODI,n.CcCod as CENCO2_CODI,convert(varchar, n.fecha,103) as FECHA,m.Facturacion as FACTURACION&#xA;&#xA;&#xA;from #facturacion  as m right join #max  as n on  m.CcCod=n.CcCod and m.Fecha=n.fecha&#xA;&#xA;&#xA;--------------------------------------outsourcing-------------------------------------&#xA;&#xA;&#xA;&#xA;--OBTENEMOS LA FACTURACION MENSUAL POR CENTRO DE COSTO&#xA;&#xA;select&#xA;&#xA;CAST (DATEADD(month, DATEDIFF(month, 0, cpbfec), 0)as DATE) as Fecha ,&#xA;CcCod,&#xA;&#xA;SUM ( case when ( pctcod like '%40-01-002%' or pctcod like '%40-01-003%' ) then MovHaberMa-MovDebeMa else 0 end ) as Facturacion&#xA;into #facturacion2&#xA;from &quot;OUTSOURCINGSA&quot;.&quot;softland&quot;.&quot;cwmovim&quot; movimientos&#xA;--where cpbano &gt;= YEAR(GETDATE())&#xA;where cpbano = YEAR(DATEADD(m,-1,DATEADD(mm, DATEDIFF(m,0,GETDATE()), 0)))  --obtiene el año del mes anterior &#xA;and (MovhaberMa&gt;0 or MovDebeMa&gt;0)&#xA;and Cccod not like '001%'&#xA;group by DATEADD(month, DATEDIFF(month, 0, cpbfec), 0),CcCod&#xA;order by  DATEADD(month, DATEDIFF(month, 0, cpbfec), 0),CcCod&#xA;&#xA;-- obtenemos la maxima fecha con facturacion&#xA;select CcCod,MAX(fecha) as fecha&#xA;into #max2&#xA;from #facturacion2&#xA;where Facturacion&gt;0&#xA;&#xA;group by CcCod&#xA;&#xA;--obtenemos la facturacion de la maxima fecha en que se facturo&#xA;&#xA;insert into Inteligencias.dbo.FACTURACION_REAL_CC&#xA;select &#xA;2 as EMP_CODI,n.CcCod as CENCO2_CODI,convert(varchar, n.fecha,103) as FECHA,m.Facturacion as FACTURACION&#xA;&#xA;&#xA;from #facturacion2  as m right join #max2  as n on  m.CcCod=n.CcCod and m.Fecha=n.fecha" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
