<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{E1898240-AA04-46F0-A1A1-AF17D90EF878}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="truncate table Inteligencias.dbo.COTIZACION_COSTOS&#xA;&#xA;insert into  Inteligencias.dbo.COTIZACION_COSTOS (Cotizacion,IdCosto,Titulo,Tipo,Monto,IdRegistro,CUENTA3_CODI,CENCO2_CODI,EMP_CODI,AREA_CODI)&#xA;&#xA;&#xA;&#xA;&#xA;SELECT        Cotizacion, IdCosto, Titulo, Tipo, Monto, row_number() OVER (ORDER BY &#xA;                             (SELECT        0)) AS IdRegistro, CASE (Tipo) &#xA;WHEN 'Sueldo Base' THEN '50-01-001' WHEN 'Anticipo Gratificación' THEN '50-01-002' WHEN 'Bono Asistencia' THEN '50-01-004' WHEN 'Bono Responsabilidad' THEN '50-01-004'&#xA; WHEN 'Bono Tercero' THEN '50-01-004' WHEN 'Bono Desgaste de Herramientas' THEN '50-01-004' WHEN 'Colación' THEN '50-01-007' WHEN 'Movilización' THEN '50-01-006' WHEN&#xA; 'Mutual de Seguridad(IST)' THEN '50-01-008' WHEN 'Prov. Indemn. Años Serv.' THEN '88-01-001' WHEN 'Provisión de Vacaciones' THEN '88-01-002' WHEN 'SeguroVida' THEN '50-01-030'&#xA; WHEN 'Invalidez y Sobrevivencia' THEN '50-01-019' WHEN 'Fest. Calendario (8hrs/mes Prom.)' THEN '50-01-023' WHEN 'Uniforme' THEN '50-01-032' WHEN 'Útiles de Escritorio'&#xA; THEN '50-01-034' WHEN 'Certificación y Credencial OS10' THEN '50-01-031' WHEN 'Seguro de Cesantía' THEN '50-01-009' &#xA; When 'Fact Precio Mensual Total' then '40-01-002' WHEN 'Fact Gasto de Administración' then '50-02-063'&#xA; END AS 'CUENTA3_CODI'&#xA; ,CENCO2_CODI,EMP_CODI,AREA_CODI&#xA; &#xA;FROM            (SELECT        [IdCosto], Costos.[Cotizacion], [Simulacion], Costos.[Columna], Costos.Titulo, [Codigo], [SueldoBase] AS 'Sueldo Base', &#xA;                                                    [Gratificacion] AS 'Anticipo Gratificación', [Bono1] AS 'Bono Asistencia', [Bono2] AS 'Bono Responsabilidad', [Bono3] AS 'Bono Tercero', &#xA;                                                    [BonoHerramienta] AS 'Bono Desgaste de Herramientas', [Movilización], [Colación], [Liquido] AS 'Líquido a Pago', Costos.Mutual AS 'Mutual de Seguridad(IST)',&#xA;                                                     [Indemnizacion] AS 'Prov. Indemn. Años Serv.', [Vacaciones] AS 'Provisión de Vacaciones', [SeguroVida], [Invalidez] AS 'Invalidez y Sobrevivencia', &#xA;                                                    [Festivo] AS 'Fest. Calendario (8hrs/mes Prom.)', [Uniforme], [Utiles] AS 'Útiles de Escritorio', [OS10] AS 'Certificación y Credencial OS10', &#xA;                                                    Costos.Cesantia AS 'Seguro de Cesantía', CONVERT(money, [Dotacion]) AS Dotacion, &#xA;                                                    [OtroGasto]/*,[Segmento] , CONVERT(money, [PorcentajeIAS]) &#xA;                                                    AS PorcentajeIAS */ , [CostoColacion], &#xA;                                                    Fact.Cotizacion AS IdCotizacion, Fact.Columna AS FactColumna, Fact.CostoEmpresa AS 'Fact Costo Mano de Obra Empresa', &#xA;                                                    Fact.Utilidad AS 'Fact Utilidad', Fact.Gasto AS 'Fact Gasto de Administración', Fact.Costo AS 'Fact Precio de Venta Unitario', &#xA;                                                    Fact.Venta AS 'Fact Precio Mensual Total', CONVERT(money, Porc.Gasto) AS 'Porc Gastos Generales %', CONVERT(money, Porc.Utilidad) &#xA;                                                    AS 'Porc Utilidad %', CASE WHEN Costos.Titulo = 'Totales' AND porc.Ordinaria &gt; 0 THEN 0 ELSE porc.Ordinaria END AS 'Porc Valor Hora Ord. Venta Neta', &#xA;                                                    CASE WHEN Costos.Titulo = 'Totales' AND porc.Extra &gt; 0 THEN 0 ELSE porc.Extra END AS 'Porc Valor Hora Extra Venta Neta', CONVERT(money, [Dotacion]) &#xA;                                                    * Fact.Costo AS 'Precio Venta Mano Obra'&#xA;                                                    ,convert(money,round(Fact.Venta*1.19,0)) as 'Fact Precio Mensual Total c/IVA'&#xA;                                                   ,COTIZACION.Centro as CENCO2_CODI,COTIZACION.Empresa as EMP_CODI,(select top 1 area_codi from inteligencias.dbo.mantenedor_cc_area  as mant where  mant.cenco2_codi collate Modern_Spanish_CI_AS=COTIZACION.Centro and mant.emp_codi=cotizacion.Empresa ) as AREA_CODI&#xA;                                                   &#xA;                          FROM            Inteligencias.dbo.COTIZACIONES_COSTOS AS Costos&#xA;                          inner join dbo.Cotizacion on Costos.Cotizacion=Cotizacion.IdCotizacion&#xA;                        &#xA;                           LEFT JOIN&#xA;                                                    Inteligencias.dbo.COTIZACION_FACTURACION AS Fact ON Costos.Cotizacion = Fact.Cotizacion AND costos.Columna = Fact.Columna LEFT JOIN&#xA;                                                    Inteligencias.dbo.COTIZACIONES_PORCENTAJE AS Porc ON Fact.Cotizacion = Porc.Cotizacion AND Fact.Columna = Porc.Columna&#xA;                                                    where Cotizacion.Estado in (0, 1, 9, 19) --and Cotizacion.IdCotizacion=2311&#xA;                                                   )  num1 &#xA;                                                   &#xA;                                                   &#xA;                         UNPIVOT  (Monto FOR Tipo IN ([Sueldo Base], [Anticipo Gratificación], [Bono Asistencia], [Bono Responsabilidad], [Bono Tercero], &#xA;                         [Bono Desgaste de Herramientas], [Movilización], [Colación], [Líquido a Pago], [Mutual de Seguridad(IST)], [Prov. Indemn. Años Serv.], [Provisión de Vacaciones], &#xA;                         [SeguroVida], [Invalidez y Sobrevivencia], [Fest. Calendario (8hrs/mes Prom.)], [Uniforme], [Útiles de Escritorio], [Certificación y Credencial OS10], &#xA;                         [Seguro de Cesantía], [Dotacion], [OtroGasto]/* ,[Segmento], [PorcentajeIAS]*/ , [CostoColacion], [Fact Costo Mano de Obra Empresa], [Fact Utilidad], &#xA;                         [Fact Gasto de Administración], [Fact Precio de Venta Unitario], [Fact Precio Mensual Total], [Porc Gastos Generales %], [Porc Utilidad %], &#xA;                         [Porc Valor Hora Ord. Venta Neta], [Porc Valor Hora Extra Venta Neta], [Precio Venta Mano Obra],[Fact Precio Mensual Total c/IVA])) AS num2&#xA;                         " SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
