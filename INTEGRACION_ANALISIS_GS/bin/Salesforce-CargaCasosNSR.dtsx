<property id="61" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">-- se definió que la deuda sea mayor a 120000

select 
id as IdCuenta
,'SIN SEÑALES' as 'Asunto'
,'Cliente con más de 3 días con NSR ('+convert(varchar,convert(int,SUBSTRING(Estado_recepciones_monitoreo__c,1,2)))+'+)' as Descripción
,'NSR' as Motivo_del_caso
,'Sin Señales (Nsr)' as Descripcion_trabajos
,'Reparacion' as Tipo_de_caso
,Estado_recepciones_monitoreo__c
  ,Casos_tecn_abiertos__c
  ,Deuda__c
  ,Estado_Cuenta_Monitoreo__c
  ,Estado_de_cuenta_facturaci_n__c,Estado_de_cuenta__c
  ,Casos_tecn_3_meses__c
  ,'012C0000000MkhTIAS' as RecordType
  ,'Servicio técnico' as 'Area'
  ,'Alta' as 'Prioridad'
  ,'Central de Monitoreo' as 'Origen del caso'
  
  
 from Inteligencias.dbo.SF_CUENTAS 

where Estado_recepciones_monitoreo__c like '%días con NSR%' and Casos_tecn_abiertos__c=0 and Deuda__c&lt;120000

and convert(int,SUBSTRING(Estado_recepciones_monitoreo__c,1,2))&gt;3 and Estado_Cuenta_Monitoreo__c='ACTIVA'</property>
