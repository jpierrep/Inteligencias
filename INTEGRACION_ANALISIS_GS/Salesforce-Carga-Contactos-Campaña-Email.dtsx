<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">select distinct Email
-- se trae el id del primer contacto salesforce para ese email (ya que el email puede estar repetido en varios contactos)  ordenado según su ultima actividad (para priorizar los contactos activos)
,(select top 1 ID from Inteligencias.dbo.SF_CONTACTOS as con where con.Email=SF_CONTACTOS.Email order by con.LastActivityDate desc)  as IdContacto
 ,'Alarmas JR' as Campaña
 from
Inteligencias.dbo.SF_CONTACTOS
where 
SF_CONTACTOS.Email is not null and
SF_CONTACTOS.Email!='xx@xx.cl' and
AccountId in
(
--Cuentas sin NSR que tengan panel JR y estén activas
SELECT 
      distinct SF_CUENTAS.id
  FROM [Inteligencias].[dbo].[SG_CUENTAS] 
  left join Inteligencias.dbo.SF_CUENTAS on case when ISNUMERIC(cue_ncuenta)=1 then convert(int,cue_ncuenta) else 0 end=  case when ISNUMERIC(AccountNumber)=1 then convert(int,AccountNumber) else 0 end
  
   where Panel_desc like '%JR%' and cue_Estado_Desc='ACTIVA' and Categoria_dias_NSR='0 DIAS'  and SF_CUENTAS.Id is not null 
   )</property>
